  "name": "clean-blog",
  "title": "Clean Blog",
  "version": "3.3.7+1",
  "homepage": "http://startbootstrap.com/template-overviews/clean-blog",
  "author": "Start Bootstrap",
  "license": {
    "type": "MIT",
    "url": "https://github.com/BlackrockDigital/startbootstrap/blob/gh-pages/LICENSE"
  },
  "devDependencies": {
    "bootstrap": "^3.3.7",
    "browser-sync": "^2.13.0",
    "compress-images": "^1.9.3",
    "csso": "^4.0.3",
    "font-awesome": "^4.6.3",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-header": "^1.8.7",
    "gulp-imagemin": "^7.1.0",
    "gulp-less": "^3.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.4",
    "jquery": "^3.4.1"
  },
  "scripts": {
    "css:lint": "npx --yes stylelint@15.11.0 \"css/**/*.css\" --config .stylelintrc.json --ignore-pattern \"**/*.min.css\" --ignore-pattern \"css/clean-blog*.css\" --allow-empty-input",
    "css:lint:fix": "npx --yes stylelint@15.11.0 \"css/**/*.css\" --config .stylelintrc.json --ignore-pattern \"**/*.min.css\" --ignore-pattern \"css/clean-blog*.css\" --fix --allow-empty-input",
    "css:unused": "npx --yes purgecss@6.0.0 --css \"css/**/*.css\" --content \"index.html\" \"pages/**/*.html\" \"posts/**/*.html\" \"templates/**/*.html\" \"js/**/*.js\" --rejected --rejected-css > output/purgecss-report.json",
    "css:purge": "npx --yes purgecss@6.0.0 --config purgecss.config.cjs",
    "css:purge:apply": "node tools/apply-purged-css.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BlackrockDigital/startbootstrap-clean-blog.git"
  }
}
